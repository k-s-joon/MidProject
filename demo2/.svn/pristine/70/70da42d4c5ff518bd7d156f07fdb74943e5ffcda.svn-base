<%@page import="notice.vo.NoticeVO"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	NoticeVO nv = (NoticeVO) request.getAttribute("nv");
// 	int fNo = (Integer) request.getAttribute("f_No");
// 	String mem_id = (String) request.getAttribute("mem_id");
%>

<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>공지사항</title>
    <link rel="stylesheet" href="../css/css.css">
</head>
<body>
<form action="./update.do" method="post" enctype="multipart/form-data">
    <div class="board_wrap">
        <div class="board_title">
            <strong>공지사항</strong>
            <p>공지 글 올리기</p>
        </div>
        <div class="board_view_wrap">
            <div class="board_view">
                <div class="title">
             	<dd><%=nv.getNoticeTitle() %></dd>
                </div>
                <div class="info">
                    <dl>
                        <dt>글 번호</dt>
                        <dd><%=nv.getNoticeNo() %></dd>
                    </dl>
                    <dl>
                        <dt>작성일</dt>
                        <dd><%=nv.getNoticeDate() %></dd>
                    </dl>
                </div>
                <div class="cont">
                  <dd><%=nv.getNoticeCon() %></dd>
                </div>
            </div>
            
            <div class="bt_wrap">
              <a href="./list.do" class="on">목록</a>   
              <a href="<%=request.getContextPath()%>/notice/update.do?noticeNo=<%=nv.getNoticeNo()%>">수정</a>
            </div>
        </div>
    </div>
</form>    
</body>
</html>