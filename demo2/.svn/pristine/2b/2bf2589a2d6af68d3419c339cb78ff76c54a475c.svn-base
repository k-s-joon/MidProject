<%@page import="free.vo.FreeVO"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
	
<%
	
	String memId = (String) session.getAttribute("loginCode");
	String reportId = (String) request.getAttribute("reportId"); 
	
%>
<!DOCTYPE html>
<html lang="ko">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>자유게시판</title>
<link rel="stylesheet" href="/css/css.css">
</head>
<body>
	<form action="MemberReport" method="post">

		<div class="board_wrap">
			<div class="board_title">
				<strong> 신고 </strong>
			</div>
			<div class="board_write_wrap">
				<div class="board_write">
					<div class="title">
						<dl>
							<dt>제목</dt>
							<dd>
								<input type="text" name="title" placeholder="제목 입력">
							</dd>
						</dl>
					</div>
					<div class="info">
						<dl>
							<dt>신고자ID</dt>
							<dd>
								<input type="text" name="goodMemId" value="<%=memId%>" readonly />
							</dd>
						</dl>
						
						<dl>
							<dt>신고하는사람ID</dt>
							<dd>
								<input type="text" name="badMemId" value="<%=reportId%>" readonly />
							</dd>
						</dl>
					</div>
					<div class="cont">
						<textarea name="content" placeholder="내용 입력"></textarea>
					</div>

					<div class="bt_wrap">
						<input type="submit" value="등록">
						<button type="button" onclick="location.href='main.jsp' ">취소</button>
					</div>
				</div>
			</div>
	</form>
</body>
</html>