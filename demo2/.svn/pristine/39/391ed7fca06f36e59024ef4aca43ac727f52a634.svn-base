     function modelOpen(memId) {
    	    	 
    	 $.ajax({
    			url : "/demo2/membersearch",
    			type : 'post',
    			data : {
    				"memId" : memId
    			},
    			success : function(data) {
    				alert("success");
    				$('.profile').html(data);
    				$('#reportId').attr('href',"MemberReport?reportId="+memId);
    		     },
    			error : function() {
    				alert("error");
    			}
    		});
     }
     
     
     
