<%@page import="free.vo.FreeVO"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
	
<%
	FreeVO fv = (FreeVO) request.getAttribute("fv");
	String memId = (String) session.getAttribute("loginCode");
	String memNick = (String) session.getAttribute("memNick");
%>
<!DOCTYPE html>
<html lang="ko">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>자유게시판</title>
<link rel="stylesheet" href="../css/css.css">
</head>
<body>
	<form action="./insert.do" method="post">
	<input type="hidden" name="memId" value="<%=memId%>">

		<div class="board_wrap">
			<div class="board_title">
				<strong>자유 게시판</strong>
				<p>게시글 올리기</p>
			</div>
			<div class="board_write_wrap">
				<div class="board_write">
					<div class="title">
						<dl>
							<dt>제목</dt>
							<dd>
								<input type="text" name="freeTitle" placeholder="제목 입력">
							</dd>
						</dl>
					</div>
					<div class="info">
						<dl>
							<dt>닉네임</dt>
							<dd>
								<input type="text" name="freeNick" value="<%=memNick%>" readonly />
							</dd>
						</dl>
					</div>
					<div class="cont">
						<textarea name="freeCon" placeholder="내용 입력"></textarea>
					</div>

					<div class="bt_wrap">
						<input type="submit" value="등록">
						<button type="button" onclick="location.href='./list.do' ">취소</button>
					</div>
				</div>
			</div>
	</form>
</body>
</html>