<%@page import="ask.vo.AskVO"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	AskVO av = (AskVO) request.getAttribute("av");
// 	int fNo = (Integer) request.getAttribute("f_No");
// 	String mem_id = (String) request.getAttribute("mem_id");
%>

<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>문의사항</title>
    <link rel="stylesheet" href="../css/css.css">
</head>
<body>
<form action="./update.do" method="post" enctype="multipart/form-data">

    <div class="board_wrap">
        <div class="board_title">
            <strong>문의사항</strong>
            <p>문의 글 올리기</p>
        </div>
        <div class="board_view_wrap">
            <div class="board_view">
                <div class="title">
             	<dd><%=av.getAskTitle() %></dd>
                </div>
                <div class="info">
                    <dl>
                        <dt>글 번호</dt>
                        <dd><%=av.getAskNo() %></dd>
                    </dl>
                    <dl>
                        <dt>작성일</dt>
                        <dd><%=av.getAskDate()%></dd>
                    </dl>
                </div>
                <div class="cont">
                  <dd><%=av.getAskCon() %></dd>
                </div>
            </div>
            
            <div class="bt_wrap">
             <a href="./list.do">[목록]</a>
			<a href="<%=request.getContextPath() %>/ask/update.do?askNo=
			<%=av.getAskNo() %>">[회원정보 수정]</a>
			<a href="./delete.do?askNo=<%=av.getAskNo() %>">[회원정보 삭제]</a>
            </div>
        </div>
    </div>
</form>    
</body>
</html>