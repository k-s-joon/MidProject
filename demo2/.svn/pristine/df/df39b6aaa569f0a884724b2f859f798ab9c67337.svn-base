<%@page import="free.vo.FreeVO"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	FreeVO fv = (FreeVO) request.getAttribute("fv");
	String memId = (String) session.getAttribute("memId");
	String memNick = (String) session.getAttribute("memNick");
%>
<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>공지사항</title>
    <link rel="stylesheet" href="../css/css.css">
</head>
<body>
<form action="./update.do" method="post">
    <div class="board_wrap">
        <div class="board_title">
            <strong>자유게시판</strong>
            <p>여러분의 이야기를 들려주세요</p>
        </div>
        <div class="board_view_wrap">
            <div class="board_view">
                <div class="title">
             	<dd><%=fv.getfTitle() %></dd>
                </div>
                <div class="info">
                    <dl>
                        <dt>번호</dt>
                        <dd><%=fv.getfNo() %></dd>
                    </dl>
                    <dl>
                        <dt>닉네임</dt>
                        <dd><%=fv.getfNick() %></dd>
                    </dl>
                    <dl>
                        <dt>작성일</dt>
                        <dd><%=fv.getfDate() %></dd>
                    </dl>
                </div>
                <div class="cont">
                  <dd><%=fv.getfCon() %></dd>
                </div>
            </div>
            <div class="rep">
                  <dd>댓글</dd>
            </div>
            <div class="bt_wrap">
                <a href="./list.do" class="on">목록</a>   
              <a href="<%=request.getContextPath()%>/free/update.do?fNo=<%=fv.getfNo()%>">수정</a>
              <a href="./delete.do?fNo=<%=fv.getfNo() %>">글 삭제</a>
            </div>
        </div>
    </div>
</form>
</body>
</html>